{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "actions/*": "./src/app/*",
      "assets/*": "./src/app/*",
      "components/*": "./src/app/*",
      "middleware/*": "./src/app/*",
      "reducers/*": "./src/app/*",
      "routes/*": "./src/app/*",
      "utils/*": "./src/app/*"
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
